  {{#if is_print}}
  {{#if mathjax_support}}
  <script>
    window.addEventListener('load', function () {
      MathJax.Hub.Register.StartupHook('End', function () {
        window.setTimeout(window.print, 100);
      });
    });
  </script>
  {{else}}
  <script>
    window.addEventListener('load', function () {
      window.setTimeout(window.print, 100);
    });
  </script>
  {{/if}}
  {{/if}}
  <script>
    document.addEventListener("DOMContentLoaded", function () {
      const searchInput = document.getElementById("searchbar");
      const searchResults = document.getElementById("searchresults");
      const searchResultsOuter = document.querySelector(".searchresults-outer-data");

      function toggleSearchResults() {
        // Check if searchresults has any child elements
        if (searchInput.value.trim() != "") {
          searchResultsOuter.style.display = "block"; // Show if there are results
        } else {
          searchResultsOuter.style.display = "none"; // Hide if there are no results
        }
      }

      // Example event listener for search input
      searchInput.addEventListener("input", function () {
        // Perform your search logic here and populate searchResults
        // This is just a placeholder for demonstration. Adjust as needed.

        // After updating searchResults, call toggleSearchResults
        toggleSearchResults();
      });

      // Initial check
      toggleSearchResults();

    });
    document.querySelectorAll('.clip-button').forEach(button => {
      button.addEventListener('click', () => {
        const codeBlock = button.closest('pre').querySelector('code');
        const text = codeBlock.innerText;

        navigator.clipboard.writeText(text).then(() => {
          const tooltip = button.querySelector('.tooltiptext');
          tooltip.textContent = 'Copied!';

          setTimeout(() => {
            tooltip.textContent = 'Copy to clipboard';
          }, 2000);
        });
      });
    });
  </script>
  <script>
    document.addEventListener('DOMContentLoaded', function () {
      function generateBreadcrumbs() {
        const path = window.location.pathname;
        // Remove leading and trailing slashes and 'index.html'
        const cleanPath = path.replace(/^\/|\/index\.html$|\/$/g, '');

        // Split path into components
        const components = cleanPath.split('/').filter(Boolean);

        // Get the breadcrumb list element
        const breadcrumbList = document.getElementById('breadcrumb-list');

        // Always add home link
        let html = '<li><a href="../../index.html"><i class="fa fa-home"></i></a></li>';

        // Build the path progressively
        let currentPath = '../../';

        components.forEach((component, index) => {
          // Format the component for display (replace hyphens with spaces, capitalize)
          const displayName = component
            .replace(/-/g, ' ')
            .replace(/\b\w/g, l => l.toUpperCase());

          currentPath += component + '/';

          if (index === components.length - 1) {
            // Current page (last item)
            html += `<li><span class="current">${displayName.replace(".Html", '')}</span></li>`;
          } else {
            // Navigation link
            html += `<li><a href="${currentPath}index.html">${displayName}</a></li>`;
          }
        });

        breadcrumbList.innerHTML = html;
      }

      // Generate breadcrumbs on page load
      generateBreadcrumbs();

      // Optional: Update breadcrumbs when the page changes (if you're using client-side routing)
      window.addEventListener('popstate', generateBreadcrumbs);
    });
  </script>
